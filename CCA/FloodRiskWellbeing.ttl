@prefix iop: <https://w3id.org/iadopt/ont/> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix ex: <http://example.org/iadopt/challenge/> .
@prefix iopp: <https://w3id.org/iadopt/pattern/> .

ex:FloodRiskWellbeing
    a 
        iop:Variable ;
    rdfs:label 
        "Flood risk index related to wellbeing" ;
    skos:definition 
        """Flood risk index related to well being of residence when water is entering the house on the first floor""" ;
    rdfs:comment 
        """Flood risk; index; well being of residence; water is entering the house on the first floor""" ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP1.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP65.yaml> ;
    iopp:pattern <https://github.com/mabablue/I-ADOPT-patterns-playground/blob/main/pattern/VDP70.yaml> ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/99" ;
    iop:hasObjectOfInterest 
        _:b0 ;
    iop:hasProperty 
        _:b1 ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "condition: water is entering the house on the first floor" ;
             iop:constrains _:b0 ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "type: well being of residence" ;
             iop:constrains _:b0 ;
        ] .

_:b0
    a 
        iop:Entity ;
    rdfs:label 
        "flood risk" .

_:b1
    a 
        iop:Property ;
    rdfs:label 
        "index" .
