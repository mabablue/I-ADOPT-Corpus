<http://example.org/iadopt/healthcare/weight>
    a 
        iop:Variable ;
    rdfs:label 
        "Weight without clothes" ;
    skos:definition
        """current weight in kilogram without clothes; in case of pregnancy: last weight before pregnancy""" ;
    rdfs:comment
        "patient; mass; condition: without clothes; time: current; conditional: if pregnant, last weight before pregnancy" ;
    ex:issue "https://github.com/mabablue/I-ADOPT-examples-playground/issues/114" ;
    iop:hasObjectOfInterest 
        <https://www.wikidata.org/entity/Q181600> ;
    iop:hasProperty 
        <https://www.wikidata.org/entity/Q11423> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "condition: without clothes" ;
             iop:constrains <https://www.wikidata.org/entity/Q181600> ;
        ] ,
        [ a iop:Constraint ;
             rdfs:label "time: last unpregnant" ;
             iop:constrains <https://www.wikidata.org/entity/Q11423> ;
        ] .

