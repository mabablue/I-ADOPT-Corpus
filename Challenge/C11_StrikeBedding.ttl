ex:C11
    a 
        iop:Variable ;
    rdfs:label 
        "Strike of bedding" ;
    rdfs:comment 
        """Geographic azimuth (relative to true north) at a point observation location, of a horizontal line contained in a sedimentary rock bedding surface. The bedding surface must not be horizontal; the azimuth is reported such that the dip direction of the inclined bedding is to the right when facing in the azimuth direction.""" ;
    iop:hasObjectOfInterest 
        _:b0 ;
    iop:hasMatrix 
        <http://opendata.inrae.fr/thesaurusINRAE/c_5217> ;
    iop:hasProperty 
        <http://qudt.org/vocab/quantitykind/Azimuth> ;
    iop:hasConstraint 
        [ a iop:Constraint ;
             rdfs:label "interpretation: angle measured with the dip direction to the right" ;
             iop:constrains <http://qudt.org/vocab/quantitykind/Azimuth> ;
        ] .

